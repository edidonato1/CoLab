\"\n              icon={faSearch} />\n            <input\n              name=\"user-search\"\n              id=\"user-search\"\n              type=\"text\"\n              value={searchUser}\n              placeholder=\" we'll search as you type\"\n              onChange={(e) => setSearchUser(e.target.value)}\n            />\n          </div>\n        </form>\n        <div className=\"searched-users\">\n          <ul id=\"user-options\">\n            {showUsers?.map(user =>\n              <li\n                key={user.id}\n                className=\"user-option\"\n                onClick={() => setOpen(user)}\n              >{user.username}</li>\n            )}\n            {searchUser.length && !showUsers.length ?\n              <li>no users match</li>\n              :\n              <></>\n            }\n          </ul>\n        </div>\n      </div>\n\n    </UserAside>\n  )\n}","/Users/edwarddidonato/Documents/Coding/sandbox/CoLab/client/src/components/aside/ProfileAside.jsx",[],"/Users/edwarddidonato/Documents/Coding/sandbox/CoLab/client/src/components/aside/UserAside.jsx",[],{"ruleId":"395","replacedBy":"396"},{"ruleId":"397","replacedBy":"398"},{"ruleId":"395","replacedBy":"399"},{"ruleId":"397","replacedBy":"400"},{"ruleId":"395","replacedBy":"401"},{"ruleId":"397","replacedBy":"402"},{"ruleId":"395","replacedBy":"403"},{"ruleId":"397","replacedBy":"404"},{"ruleId":"405","severity":1,"message":"406","line":19,"column":9,"nodeType":"407","messageId":"408","endLine":19,"endColumn":13},{"ruleId":"405","severity":1,"message":"409","line":10,"column":10,"nodeType":"407","messageId":"408","endLine":10,"endColumn":15},{"ruleId":"405","severity":1,"message":"406","line":4,"column":9,"nodeType":"407","messageId":"408","endLine":4,"endColumn":13},{"ruleId":"405","severity":1,"message":"406","line":8,"column":9,"nodeType":"407","messageId":"408","endLine":8,"endColumn":13},{"ruleId":"405","severity":1,"message":"406","line":17,"column":9,"nodeType":"407","messageId":"40