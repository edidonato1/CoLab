ult()}>\n        <input\n          type=\"text\"\n          value={searchUser}\n          onChange={(e) => setSearchUser(e.target.value)}\n        />\n        <select onChange={(e) => setMediaFilter([...mediaFilter, e.target.value])}>\n        <option disabled value='default' >find by media</option>\n          {media.map(medium => \n            <option value={medium.name}>{ medium.name}</option>\n            )}\n        </select>\n      </form>\n      {showUsers?.map(user =>\n        <div className=\"user-thumbnail\">\n          <Link  className=\"user-link\" key={user.id} to={`/users/${user.id}`}>\n            <img  className=\"user-thumbnail-img\" src={ user.img_url ? user.img_url : `https://images.unsplash.com/photo-1569172122301-bc5008bc09c5?ixid=MXwxMjA3fDB8MHxzZWFyY2h8Mnx8YXJ0fGVufDB8fDB8&ixlib=rb-1.2.1&auto=format&fit=crop&w=800&q=60`}/>\n            <p className=\"thumbnail-username\">{user.username}</p>\n            <p>matching media:</p>\n            {user.media.filter(medium => \n              mediaFilter.includes(medium.name)\n            ).map(medium => \n              <p>{ medium.name}</p>\n              )}\n          </Link>\n        </div>\n      )}\n    </UserList>\n  )\n}","/Users/edwarddidonato/Documents/Coding/sandbox/CoLab/client/src/screens/user/UserDetail.jsx",["252","253"],"/Users/edwarddidonato/Documents/Coding/sandbox/CoLab/client/src/screens/medium/Media.jsx",[],"/Users/edwarddidonato/Documents/Coding/sandbox/CoLab/client/src/services/media.js",["254"],"/Users/edwarddidonato/Documents/Coding/sandbox/CoLab/client/src/components/Layout.jsx",[],"/Users/edwarddidonato/Documents/Coding/sandbox/CoLab/client/src/screens/user/Login.jsx",[],"/Users/edwarddidonato/Documents/Coding/sandbox/CoLab/client/src/screens/user/Profile.jsx",[],"/Users/edwarddidonato/Documents/Coding/sandbox/CoLab/client/src/services/posts.js",["255","256"],"/Users/edwarddidonato/Documents/Coding/sandbox/CoLab/client/src/stylesheets/FormStyles.js",[],"/Users/edwarddidonato/Doc